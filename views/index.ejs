<!DOCTYPE html>
<html>
<head>
    <title>Angular JS</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="javascripts/angular.min.js"></script>
  <script type="text/javascript" src="javascripts/angular.js"></script>
</head>
<body ng-app="myApp">
  <div class="container" ng-controller="myController">
    <form class="form-group">
        <h1>Form</h1>
        <label>Name :</label>
        <input class="form-control" type="text" ng-model="users.name">
        <label>Roll No :</label>
        <input class="form-control" type="text" ng-model="users.rollno">
        <label>Branch :</label>
        <input class="form-control" type="text" ng-model="users.branch">
        <label>College :</label>
        <input class="form-control" type="text" ng-model="users.college">
        <br>
        <button class="btn btn-info" ng-click="saveData(users)">Submit</button>
    </form>
    <br>
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>S.No</th>
          <th>Name</th>
          <th>Rollno</th>
          <th>Branch</th>
          <th>College</th>
          <th>Remove</th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat="users in user">
          <td>{{$index+1}}</td>
          <td>{{users.name}}</td>
          <td>{{users.rollno}}</td>
          <td>{{users.branch}}</td>
          <td>{{users.college}}</td>
          <td>
            <i class="btn btn-danger glyphicon glyphicon-trash" ng-click="removeData($index,users)"></i>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</body>
</html>